			<Button name="OpenMyMailMoneyButton" inherits="ItemButtonTemplate">
				<Scripts>
					<OnLoad>
						self:RegisterEvent("MAIL_INBOX_UPDATE");
						self:RegisterForClicks("LeftButtonUp", "RightButtonUp");
					</OnLoad>
					<OnEvent>
						--GameTooltip:Hide();
						if ( GameTooltip:IsOwned(self) ) then
							if ( OpenMyMailFrame.money ) then
								GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
								SetTooltipMoney(GameTooltip, OpenMyMailFrame.money);
								GameTooltip:Show();
							end
						end
					</OnEvent>
					<OnClick>
						OpenMyMailFrame.updateButtonPositions = false;
						TakeInboxMoney(MyInboxFrame.openMailID);
					</OnClick>
					<OnEnter>
						if ( OpenMyMailFrame.money ) then
							GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
							SetTooltipMoney(GameTooltip, OpenMyMailFrame.money);
							GameTooltip:Show();
						end
					</OnEnter>
					<OnLeave function="GameTooltip_Hide"/>
